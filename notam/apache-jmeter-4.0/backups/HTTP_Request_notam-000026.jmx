    public void foo(byte[] arr, int size){

        long polynomial = 0x8408;
        long CRC = 0;

        for (byte b : arr)
        {
            CRC ^= b &amp; 0xff;
            for (int i = 8; i != 0; i--)
            {
                if ((CRC &amp; 0x0001) != 0)
                {
                    CRC = (CRC &gt;&gt; 1) ^ polynomial;
                }
                else
                {
                    CRC &gt;&gt;= 1;
                }
            }
        }

        System.out.println(Long.toHexString(CRC));
    }

